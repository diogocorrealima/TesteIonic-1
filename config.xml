    <plugin name="cordova-plugin-statusbar" spec="2.4.2" />
    <plugin name="cordova-plugin-device" spec="2.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="5.0.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="^3.0.0" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.0.5" />
    <plugin name="cordova-plugin-firebase" spec="2.0.5" />
    <plugin name="cordova-plugin-firebase-authentication" spec="1.0.1">
        <variable name="FIREBASE_AUTH_VERSION" value="16.0.+" />
    </plugin>
    <plugin name="cordova-plugin-firebase-config" spec="1.1.0">
        <variable name="FIREBASE_CORE_VERSION" value="16.0.+" />
        <variable name="FIREBASE_CONFIG_VERSION" value="16.3.+" />
    </plugin>
    <plugin name="cordova-plugin-fingerprint-aio" spec="1.6.0" />
    <plugin name="cordova-support-google-services" spec="1.2.1" />
    <plugin name="cordova-plugin-add-swift-support" spec="1.7.2" />
